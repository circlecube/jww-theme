{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "jww/song-play-history",
    "version": "1.0.0",
    "title": "JWW Song Play History",
    "category": "media",
    "icon": "list-view",
    "description": "Display all times a song was played live: list or sortable table (date, location, venue, set position, days since previous).",
    "keywords": [
        "song",
        "play",
        "history",
        "setlist",
        "shows"
    ],
    "textdomain": "jww-theme",
    "editorScript": "file:../../build/index.js",
    "editorStyle": "file:../../build/index.css",
    "style": "file:../../build/style-index.css",
    "render": "file:./render.php",
    "supports": {
        "align": [
            "wide",
            "full"
        ],
        "spacing": {
            "padding": true,
            "margin": true
        },
        "typography": {
            "fontSize": true,
            "lineHeight": true
        }
    },
    "attributes": {
        "displayMode": {
            "type": "string",
            "default": "table",
            "enum": [
                "list",
                "table"
            ]
        },
        "songId": {
            "type": "number",
            "default": 0
        },
        "limit": {
            "type": "number",
            "default": 0
        }
    }
}