{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "jww/tour-timeline",
    "version": "1.0.0",
    "title": "Tour Timeline",
    "category": "media",
    "icon": "calendar-alt",
    "description": "Display a timeline of shows for a specific tour.",
    "keywords": [
        "tour",
        "timeline",
        "shows",
        "dates"
    ],
    "textdomain": "jww-theme",
    "editorScript": "file:../../build/index.js",
    "editorStyle": "file:../../build/index.css",
    "style": "file:../../build/style-index.css",
    "render": "file:./render.php",
    "supports": {
        "align": [
            "wide",
            "full"
        ],
        "spacing": {
            "padding": true,
            "margin": true
        }
    },
    "attributes": {
        "tourId": {
            "type": "number",
            "default": 0
        },
        "showPastOnly": {
            "type": "boolean",
            "default": false
        },
        "showUpcomingOnly": {
            "type": "boolean",
            "default": false
        }
    }
}
